{
  "manifest_version": 3,
  "name": "Osu score calculator",
  "description": "Allows you to determine how many pp you will get for score",
  "version": "1.0",

  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "osu-score-calculator-logo.png"
  },
  "icons": {
    "16": "osu-score-calculator-logo-16.png",
    "48": "osu-score-calculator-logo-48.png",
    "128": "osu-score-calculator-logo-128.png"
  }
}
